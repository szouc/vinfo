    "webpack-node-externals": "^1.6.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      [
        "flow-react-proptypes"
      ]
    ]
  },
  "browserslist": [
    "> 1%"
  ],
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "bail": true,
    "verbose": true
  }
}
