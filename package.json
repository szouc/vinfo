{
  "name": "vinfo",
  "version": "1.0.0",
  "description": "ER for logistics",
  "main": "index.js",
  "author": "szouc",
  "license": "MIT",
  "scripts": {
    "tmp": "babel-node ./tmp.js",
    "start": "cross-env DEBUG=app,user yarn dev:start",
    "vsc": "cross-env DEBUG=server,app,user,auth yarn dev:vsc",
    "dev": "yarn dev:dll && yarn dev:wds",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:vsc": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node --inspect=5858 src/server",
    "dev:wds": "webpack-dev-server --config webpack.config.babel.js --progress --color",
    "dev:dll": "webpack --config webpack.dll.config.babel.js -p --progress --color --hide-modules",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack --config webpack.config.babel.js -p --progress",
    "server:bundle": "cross-env NODE_ENV=production babel-node tools/serverBundle",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js webpack.dll.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "jest": "cross-env NODE_ENV=test jest --runInBand --watchAll",
    "precommit": "yarn lint",
    "prepush": "yarn prod:build"
  },
  "dependencies": {
    "antd": "^2.12.8",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.3.0",
    "cors": "^2.8.4",
    "debug": "^3.0.1",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "history": "^4.7.2",
    "immutable": "4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.5.0",
    "lodash.indexof": "^4.0.5",
    "lodash.reduce": "^4.6.0",
    "moment": "^2.18.1",
    "mongoose": "^4.11.9",
    "morgan": "^1.8.2",
    "normalizr": "^3.2.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-loadable": "^4.0.4",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-form": "^7.0.3",
    "redux-immutable": "^4.0.0",
    "redux-persist": "^4.9.1",
    "redux-persist-immutable": "^4.3.0",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-react-proptypes": "^5.1.2",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "eslint": "4.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
